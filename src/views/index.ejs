<!DOCTYPE html>
<html lang="en">
<%- include("./partials/header.ejs") %>
<body>
    <div class="container">
        <div class="content">
    <% if (users.length > 0) { users.forEach(user => { %>
        <h3 class="title"><%= user.firstName %> <%= user.lastName %></h3>
        <p><%= user.email %></p>
    <% })}else{ %>
        <h1>não há usuários cadastrados</h1>
    <% } %>
        </div>
    </div>
</body>
</html>